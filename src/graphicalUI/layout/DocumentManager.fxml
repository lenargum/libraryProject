<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXListView?>
<?import com.jfoenix.controls.JFXTreeTableView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<StackPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.141"
           fx:controller="graphicalUI.DocumentManager">
	<children>
		<AnchorPane prefHeight="600.0" prefWidth="800.0">
			<children>
				<JFXButton fx:id="goBackBtn" mnemonicParsing="false" text="GO BACK" AnchorPane.leftAnchor="10.0"
				           AnchorPane.topAnchor="10.0"/>
				<JFXListView fx:id="documentsList" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0"
				             AnchorPane.topAnchor="50.0"/>
				<JFXTreeTableView fx:id="docsTable" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0"
				                  AnchorPane.topAnchor="50.0"/>
				<JFXButton fx:id="addDocBtn" buttonType="RAISED" disable="true" mnemonicParsing="false"
				           prefHeight="50.0" prefWidth="50.0"
				           style="-fx-background-color: #43a047; -fx-background-radius: 50;" text="+" textFill="WHITE"
				           AnchorPane.bottomAnchor="40.0" AnchorPane.rightAnchor="30.0">
					<font>
						<Font name="Roboto" size="26.0"/>
					</font>
				</JFXButton>
			</children>
		</AnchorPane>
	</children>
</StackPane>
