<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTreeTableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<StackPane xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="graphicalUI.UserManager">
   <children>
      <AnchorPane prefHeight="600.0" prefWidth="800.0">
      	<children>
      		<JFXButton fx:id="goBackBtn" mnemonicParsing="false" text="GO BACK" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="10.0" />
            <JFXButton fx:id="addDocBtn" buttonType="RAISED" mnemonicParsing="false" prefHeight="50.0" prefWidth="50.0" style="-fx-background-color: #e53935; -fx-background-radius: 50;" text="+" textFill="WHITE" AnchorPane.bottomAnchor="40.0" AnchorPane.rightAnchor="30.0">
               <font>
                  <Font name="Roboto" size="26.0" />
               </font>
            </JFXButton>
            <JFXTreeTableView AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0" AnchorPane.topAnchor="50.0" />
      	</children>
      </AnchorPane>
   </children>
</StackPane>
